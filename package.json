{
  "name": "nestjs-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json",
    "prepare": "is-ci || husky install",
    "migrate:generate": "drizzle-kit generate:pg",
    "migrate:dev": "drizzle-kit push:pg",
    "migrate:run": "npx ts-node src/modules/drizzle/run-migrations.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@arendajaelu/nestjs-passport-apple": "^2.0.3",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@google-cloud/storage": "^7.7.0",
    "@nestjs/common": "9.4.0",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.4.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "9.4.0",
    "@nestjs/swagger": "6.3.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/passport-google-oauth2": "^0.1.8",
    "@types/passport-local": "^1.0.35",
    "@types/stream-buffers": "^3.0.7",
    "@types/uuid": "^9.0.4",
    "aws-sdk": "^2.1586.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "camaro": "^6.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.9",
    "drizzle-orm": "^0.28.5",
    "fast-xml-parser": "^4.2.7",
    "google-auth-library": "^9.14.2",
    "handlebars": "^4.7.8",
    "jwks-rsa": "^3.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "8.10.0",
    "postgres": "^3.3.5",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.4.1",
    "rxjs": "7.8.0",
    "source-map-support": "0.5.21",
    "stream-buffers": "^3.0.2",
    "swagger-ui-express": "4.6.2",
    "uuid": "^9.0.1",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nestjs/cli": "9.3.0",
    "@nestjs/schematics": "9.1.0",
    "@nestjs/testing": "9.4.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.4.0",
    "@types/multer": "^1.4.11",
    "@types/node": "18.15.11",
    "@types/passport-jwt": "^3.0.13",
    "@types/pg": "^8.10.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.58.0",
    "@typescript-eslint/parser": "5.58.0",
    "drizzle-kit": "^0.19.13",
    "env-cmd": "10.1.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.5.0",
    "typescript": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
